ter Property="av:UIElement.Effect"><av:Setter.Value><av:DropShadowEffect Color="#FFF5F5DC" /></av:Setter.Value></av:Setter></av:Style></BSkyTextBox.Style></BSkyTextBox><BSkyRadioButton GroupName="a" Name="lowtail" Text="Lower tail" Width="81" Left="0" Top="0" Height="20" IsSelected="True" CanExecute="True" Enabled="True" Syntax="TRUE" IsChecked="True" FontFamily="Segoe UI" FontSize="12" Style="{av:DynamicResource RadioButtonTemplate}" IsEnabled="True" av:Canvas.Left="5" av:Canvas.Top="216"><BSkyRadioButton.CheckedChangeBehaviour><BehaviourCollection Capacity="4" /></BSkyRadioButton.CheckedChangeBehaviour>Lower tail</BSkyRadioButton><BSkyRadioButton GroupName="a" Name="uptail" Text="Upper tail" Width="78" Left="0" Top="0" Height="18" IsSelected="False" CanExecute="True" Enabled="True" Syntax="FALSE" IsChecked="False" FontFamily="Segoe UI" FontSize="12" Style="{av:DynamicResource RadioButtonTemplate}" IsEnabled="True" av:Canvas.Left="5" av:Canvas.Top="237"><BSkyRadioButton.CheckedChangeBehaviour><BehaviourCollection Capacity="4" /></BSkyRadioButton.CheckedChangeBehaviour>Upper tail</BSkyRadioButton><BSkyMultiLineLabel Name="msg" Width="268" Height="35" Left="4" Top="0" CanExecute="True" TextWrapping="Wrap" AcceptsTab="True" VerticalScrollBarVisibility="Auto" FontFamily="Segoe UI" FontSize="12" VerticalContentAlignment="Top" Template="{av:DynamicResource TextBoxControlTemplateMultiline}" av:Canvas.Left="4" av:Canvas.Top="0" xml:space="preserve"><BSkyMultiLineLabel.Style><av:Style TargetType="av:TextBox"><av:Style.Resources><av:ResourceDictionary /></av:Style.Resources><av:Setter Property="av:UIElement.Effect"><av:Setter.Value><av:DropShadowEffect Color="#FFF5F5DC" /></av:Setter.Value></av:Setter></av:Style></BSkyMultiLineLabel.Style>Enter variable value(s) separated by  a comma. Example : 3, .5</BSkyMultiLineLabel><BSkyLabelReqdField Name="rq1" Text="*" Width="15" Height="25" Left="79" Top="9" CanExecute="False" Foreground="#FFFF0000" FontFamily="Segoe UI" FontSize="12" FontWeight="Bold" av:Canvas.Left="79" av:Canvas.Top="9">*</BSkyLabelReqdField><BSkyLabelReqdField Name="rq2" Text="*" Width="15" Height="25" Left="170" Top="79" CanExecute="False" Foreground="#FFFF0000" FontFamily="Segoe UI" FontSize="12" FontWeight="Bold" av:Canvas.Left="170" av:Canvas.Top="79">*</BSkyLabelReqdField><BSkyLabelReqdField Name="rq3" Text="*" Width="15" Height="25" Left="181" Top="140" CanExecute="False" Foreground="#FFFF0000" FontFamily="Segoe UI" FontSize="12" FontWeight="Bold" av:Canvas.Left="181" av:Canvas.Top="140">*</BSkyLabelReqdField></BSkyCanvas>