+geom_point(shape  = 15,size   = 4) + geom_errorbar(aes(ymin  =  lower.CL,ymax  =  upper.CL),width =  0.2,size  =  0.7) + theme_bw() +theme(axis.title   = element_text(face = &quot;bold&quot;),axis.text    = element_text(face = &quot;bold&quot;),plot.caption = element_text(hjust = 0)) + ylab(&quot;Sodium&quot;) +geom_text(nudge_x = c(0,0,0),nudge_y = c(120,120,120),color   = &quot;black&quot;) +ggtitle(&quot;Least square means of the fixed effect (adjusted for means of other factors in model)&quot;))&#xA;}&#xA;}&#xA;)&#xA;&#xA;&#xA;&#xA;" Helpfile="" splitProcessing="True" DatasetRequired="True" MenuLocation="{x:Null}" OutputDefinition="{x:Null}" Width="500" Command="False" Height="350" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 10px } .c2 { border-color: Black; border-style: solid; border-width: 0px; border-spacing: 3px; width: 41.82% } .c3 { vertical-align: middle } .c4 { border-color: Black; border-style: solid; border-width: 0px; padding: 1px; vertical-align: middle } .c5 { margin: 0px; line-height: normal; font-size: 16px; font-family: 'Times New Roman','Times New Roman' } .c6 { font-size: 18px; font-family: 'Times New Roman','Times New Roman'; color: Black } .c7 { margin: 0px 0px 11px; line-height: normal; font-size: 16px; font-family: 'Times New Roman','Times New Roman'; background-color: White } .c8 { font-size: 16px; font-family: 'Courier New'; color: #666666; font-weight: bold } .c9 { font-size: 16px; font-family: 'Courier New'; color: #666666; text-decoration: underline; font-weight: bold } .c10 { margin: 0px 0px 11px; line-height: normal; font-size: 16px; font-family: 'Times New Roman','Times New Roman' } .c11 { font-size: 16px; font-family: 'Times New Roman','Times New Roman'; color: Black } .c12 { font-size: 13.3333333333333px; font-family: 'Courier New'; color: Black } .c13 { border-color: Black; border-style: solid; border-width: 0px; border-spacing: 3px } .c14 { border-color: Black; border-style: solid; border-width: 0px; padding: 1px; vertical-align: top } .c15 { font-style: italic } .c16 { color: Blue; text-decoration: underline } .c17 { font-size: 13.3333333333333px; font-family: 'Courier New'; color: Purple; background-color: White } .c18 { margin: 0px 0px 11px; line-height: 1.07; font-size: 16px; font-family: 'Times New Roman','Times New Roman' } .c19 { margin: 0px 0px 15px; font-size: 18px; font-weight: bold; font-family: 'Times New Roman','Times New Roman'; background-color: White } .c20 { font-size: 16px; font-family: 'Courier New'; color: #666666 } .c21 { margin: 0px; font-size: 16px; font-family: 'Times New Roman','Times New Roman' } .c22 { font-size: 18px; color: Black } .c23 { color: Black; text-decoration: underline; font-size: 13.3333333333333px; font-family: 'Courier New' } .c24 { font-size: 16px; font-family: 'Times New Roman','Times New Roman'; color: Purple; background-color: White } .c25 { font-family: 'Courier New' } .c26 { font-size: 13.3333333333333px; color: Black } .c27 { font-size: 16px; line-height: 1.07; font-family: 'Courier New'; text-decoration: underline; font-weight: bold } .c28 { font-size: 16px; line-height: 1.07; font-family: 'Courier New' } .c29 { color: Blue; text-decoration: underline; font-family: 'Courier New' } .c30 { color: Blue; text-decoration: underline; font-size: 16px; line-height: 1.07; font-family: 'Courier New' } &lt;/style&gt;&lt;/head&gt;&lt;body class=&quot;c0&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&amp;#x200b;&lt;/p&gt;&lt;table class=&quot;c2&quot;&gt;&lt;col/&gt;&lt;tbody class=&quot;c3&quot;&gt;&lt;tr class=&quot;c3&quot;&gt;&lt;td class=&quot;c4&quot;&gt;&lt;p class=&quot;c5&quot;&gt;&lt;span class=&quot;c6&quot;&gt;lmer {lme4}&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p class=&quot;c7&quot;&gt;&lt;span class=&quot;c8&quot;&gt;Fit Linear Mixed-Effects Models&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c7&quot;&gt;&lt;span class=&quot;c9&quot;&gt;Description&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c10&quot;&gt;&lt;span class=&quot;c11&quot;&gt;Fit a linear mixed-effects model (LMM) to data, via REML or maximum likelihood.&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c7&quot;&gt;&lt;span class=&quot;c9&quot;&gt;Usage&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c5&quot;&gt;&lt;span class=&quot;c12&quot;&gt;lmer(formula, data = NULL, REML = TRUE, control = lmerControl(),&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c5&quot;&gt;&lt;span class=&quot;c12&quot;&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;start = NULL, verbose = 0L, subset, weights, na.action,&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c5&quot;&gt;&lt;span class=&quot;c12&quot;&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;offset, contrasts = NULL, devFunOnly = FALSE, ...)&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c7&quot;&gt;&lt;span class=&quot;c9&quot;&gt;Arguments&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;table class=&quot;c13&quot;&gt;&lt;col/&gt;&lt;col/&gt;&lt;tbody class=&quot;c3&quot;&gt;&lt;tr class=&quot;c3&quot;&gt;&lt;td class=&quot;c14&quot;&gt;&lt;p class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;formula&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c6&quot;&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c14&quot;&gt;&lt;p class=&quot;c10&quot;&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;a two-sided linear formula object describing both the fixed-effects and random-effects part of the model, with the response on the left of a&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;~&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;&amp;#160;operator and the terms, separated by&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;+&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;operators, on the right. Random-effects terms are distinguished by vertical bars (&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;|&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;) separating expressions for design matrices from grouping factors. Two vertical bars (&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;||&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;) can be used to specify multiple uncorrelated random effects for the same grouping variable. (Because of the way it is implemented, the&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;||&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c6&quot;&gt;-syntax&amp;#160;&lt;span class=&quot;c15&quot;&gt;works only for design matrices containing numeric (continuous) predictors&lt;/span&gt;; to fit models with independent categorical effects, see&amp;#160;&lt;/span&gt;&lt;a href=&quot;http://127.0.0.1:26703/library/lme4/help/dummy&quot; class=&quot;c16&quot;&gt;&lt;span&gt;&lt;span class=&quot;c17&quot;&gt;dummy&lt;/span&gt;&lt;/span&gt;&lt;/a&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;&amp;#160;or the&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;lmer_alt&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;&amp;#160;function from the&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;afex&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c6&quot;&gt;&amp;#160;package.)&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c3&quot;&gt;&lt;td class=&quot;c14&quot;&gt;&lt;p class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;data&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c6&quot;&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c14&quot;&gt;&lt;p class=&quot;c10&quot;&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;an optional data frame containing the variables named in&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;formula&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;. By default the variables are taken from the environment from which&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;lmer&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;&amp;#160;is called. While&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;data&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c6&quot;&gt;&amp;#160;is optional, the package authors&amp;#160;&lt;span class=&quot;c15&quot;&gt;strongly&lt;/span&gt;&amp;#160;recommend its use, especially when later applying methods such as&amp;#160;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;update&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;&amp;#160;and&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;drop1&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c6&quot;&gt;&amp;#160;to the fitted model (&lt;span class=&quot;c15&quot;&gt;such methods are not guaranteed to work properly if&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c15&quot;&gt;&lt;span class=&quot;c12&quot;&gt;data&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c15&quot;&gt;&lt;span class=&quot;c6&quot;&gt;&amp;#160;is omitted&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;). If&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;data&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;&amp;#160;is omitted, variables will be taken from the environment of&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;formula&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c6&quot;&gt;&amp;#160;(if specified as a formula) or from the parent frame (if specified as a character vector).&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c3&quot;&gt;&lt;td class=&quot;c14&quot;&gt;&lt;p class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;REML&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c6&quot;&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c14&quot;&gt;&lt;p class=&quot;c10&quot;&gt;&lt;span class=&quot;c6&quot;&gt;logical scalar - Should the estimates be chosen to optimize the REML criterion (as opposed to the log-likelihood)?&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p class=&quot;c18&quot;&gt;&amp;#160;&lt;/p&gt;&lt;table class=&quot;c13&quot;&gt;&lt;col/&gt;&lt;col/&gt;&lt;tbody class=&quot;c3&quot;&gt;&lt;tr class=&quot;c3&quot;&gt;&lt;td class=&quot;c14&quot;&gt;&lt;p class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;na.action&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c6&quot;&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c14&quot;&gt;&lt;p class=&quot;c10&quot;&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;a function that indicates what should happen when the data contain&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;NA&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;s. The default action (&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;na.omit&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;, inherited from the 'factory fresh' value of&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;getOption(&amp;quot;na.action&amp;quot;)&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c6&quot;&gt;) strips any observations with any missing values in any variables.&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;div class=&quot;c19&quot;&gt;&lt;span class=&quot;c20&quot;&gt;Value&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c21&quot;&gt;&lt;span&gt;&lt;span class=&quot;c22&quot;&gt;An object of class&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;a href=&quot;http://127.0.0.1:26703/library/lme4/help/merMod-class&quot; class=&quot;c23&quot;&gt;&lt;span&gt;&lt;span class=&quot;c24&quot;&gt;merMod&lt;/span&gt;&lt;/span&gt;&lt;/a&gt;&lt;span class=&quot;c22&quot;&gt;&amp;#160;(more specifically, an object of&amp;#160;&lt;span class=&quot;c15&quot;&gt;subclass&lt;/span&gt;&amp;#160;&lt;/span&gt;&lt;span class=&quot;c25&quot;&gt;&lt;span class=&quot;c26&quot;&gt;lmerMod&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c22&quot;&gt;), for which many methods are available (e.g.&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c25&quot;&gt;&lt;span class=&quot;c26&quot;&gt;methods(class=&amp;quot;merMod&amp;quot;)&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c22&quot;&gt;)&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c21&quot;&gt;&lt;span&gt;&lt;span class=&quot;c22&quot;&gt;&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;&lt;span class=&quot;c27&quot;&gt;R Help&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;&lt;span class=&quot;c25&quot;&gt;help(lmer)&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c21&quot;&gt;&lt;span&gt;&lt;span class=&quot;c22&quot;&gt;&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c21&quot;&gt;&lt;span&gt;&lt;span class=&quot;c22&quot;&gt;&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c21&quot;&gt;&lt;span class=&quot;c22&quot;&gt;anova{ lmerTest }&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;&lt;span class=&quot;c28&quot;&gt;Description Methods for Function anova in Package lmerTest&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;&lt;span class=&quot;c27&quot;&gt;Usage&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;## S4 method for signature 'merModLmerTest' anova(object, ... , ddf=&amp;quot;Satterthwaite&amp;quot;, type=3)&lt;span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;&amp;#160;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;&lt;span class=&quot;c27&quot;&gt;Arguments&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;object:&amp;#160;object of class &amp;quot;merModLmerTest&amp;quot; ... object of class &amp;quot;merModLmerTest&amp;quot;. Then the model comparison statistisc will be calculated&lt;span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;ddf:&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;By default the Satterthwaite’s approximation to degrees of freedom is calculated. If ddf=&amp;quot;Kenward-Roger&amp;quot;, then the Kenward-Roger’s approximation is calculated using KRmodcomp function from pbkrtest package. If ddf=&amp;quot;lme4&amp;quot; then the anova table that comes from lme4 package is returned.&lt;span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;Type:&amp;#160;&amp;#160;&amp;#160;&amp;#160;type of hypothesis to be tested. Could be type=3 or type=2 or type = 1 (The definition comes from SAS theory) Details According to (Goodnight, J.H. 1976) the behaviour of the type 3 hypothesis is not fully studied for the situations with missing cells (where observations are missing at some factor-level combination). A warning is returned in such cases.&lt;span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;&lt;span&gt;&lt;span class=&quot;c28&quot;&gt;&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;&lt;span class=&quot;c27&quot;&gt;Details&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;According to (Goodnight, J.H. 1976) the behaviour of the type 3 hypothesis is not fully studied for the situations with missing cells (where observations are missing at some factor-level combination). A warning is returned in such cases.&lt;span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;&lt;span class=&quot;c27&quot;&gt;R Help&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;&lt;a href=&quot;https://cran.r-project.org/web/packages/lmerTest/lmerTest.pdf&quot; class=&quot;c29&quot;&gt;https://cran.r-project.org/web/packages/lmerTest/lmerTest.pdf&lt;/a&gt;&lt;span class=&quot;c25&quot;&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;&lt;span class=&quot;c25&quot;&gt;help(nagelkerke, package ='rcompanion')&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;&lt;span class=&quot;c25&quot;&gt;help(emmeans,package=’cld’)&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;&lt;span class=&quot;c25&quot;&gt;help(cld,package='emmeans')&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;&lt;span class=&quot;c27&quot;&gt;Reference material&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c18&quot;&gt;&lt;a href=&quot;http://rcompanion.org/handbook/I_07.html&quot; class=&quot;c30&quot;&gt;http://rcompanion.org/handbook/I_07.html&lt;/a&gt;&lt;span class=&quot;c28&quot;&gt;&lt;/span&gt;&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;" RHelpText="help(lmer, package=lme4)" Background="#FFEDEFFF" xmlns="clr-namespace:BSky.Controls;assembly=BSky.Controls" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation"><BSkySourceList CanExecute="True" Syntax="%%VALUE%%" summaryCtrl="False" PrefixTxt="{x:Null}" SepCharacter="{x:Null}" maxNoOfVariables="" SubstituteSettings="NoPrefix|UseComma" ordlevels="{x:Null}" nomlevels="{x:Null}" Name="svarbox1" AutoVar="False" renderVars="False" Filter="String|Numeric|Date|Ordinal|Nominal|Scale" Width="210" Height="314" Left="10" Top="28" Enabled="True" OverwriteSettings="{x:Null}" MoveVariables="True" SelectionMode="Extended"  AllowDrop="True" IsEnabled="True" Fo