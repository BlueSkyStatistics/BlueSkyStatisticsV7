haviour><Behaviour.Condition><Condition PropertyName="Text" Value="{assembly:Null}" Operator="isNullOrEmpty" /></Behaviour.Condition><Behaviour.Setters><PropertySettersCollection Capacity="4"><PropertySetter ControlName="shape2" PropertyName="CanExecute" Value="false" /></PropertySettersCollection></Behaviour.Setters></Behaviour><Behaviour><Behaviour.Condition><Condition PropertyName="Text" Value="{assembly:Null}" Operator="IsNumeric" /></Behaviour.Condition><Behaviour.Setters><PropertySettersCollection Capacity="4"><PropertySetter ControlName="shape2" PropertyName="CanExecute" Value="true" /></PropertySettersCollection></Behaviour.Setters></Behaviour><Behaviour><Behaviour.Condition><Condition PropertyName="Text" Value="{assembly:Null}" Operator="NotNumeric" /></Behaviour.Condition><Behaviour.Setters><PropertySettersCollection Capacity="4"><PropertySetter ControlName="shape2" PropertyName="CanExecute" Value="false" /></PropertySettersCollection></Behaviour.Setters></Behaviour></BehaviourCollection></BSkyTextBox.TextChangedBehaviour><BSkyTextBox.Style><av:Style TargetType="av:TextBox"><av:Style.Resources><av:ResourceDictionary /></av:Style.Resources><av:Setter Property="av:UIElement.Effect"><av:Setter.Value><av:DropShadowEffect Color="#FFF5F5DC" /></av:Setter.Value></av:Setter></av:Style></BSkyTextBox.Style></BSkyTextBox><BSkyRadioButton GroupName="a" Name="lowtail" Text="Lower tail" Width="81" Left="0" Top="0" Height="20" IsSelected="True" CanExecute="True" Enabled="True" Syntax="TRUE" IsChecked="True" FontFamily="Segoe UI" FontSize="12" Style="{av:DynamicResource RadioButtonTemplate}" IsEnabled="True" av:Canvas.Left="5" av:Canvas.Top="213"><BSkyRadioButton.CheckedChangeBehaviour><BehaviourCollection Capacity="4" /></BSkyRadioButton.CheckedChangeBehaviour>Lower tail</BSkyRadioButton><BSkyRadioButton GroupName="a" Name="uptail" Text="Upper tail" Width="78" Left="0" Top="0" Height="18" IsSelected="False" CanExecute="True" Enabled="True" Syntax="FALSE" IsChecked="False" FontFamily="Segoe UI" FontSize="12" Style="{av:DynamicResource RadioButtonTemplate}" IsEnabled="True" av:Canvas.Left="5" av:Canvas.Top="234"><BSkyRadioButton.CheckedChangeBehaviour><BehaviourCollection Capacity="4" /></BSkyRadioButton.CheckedChangeBehaviour>Upper tail</BSkyRadioButton><BSkyMultiLineLabel Name="msg" Width="268" Height="35" Left="4" Top="0" CanExecute="True" TextWrapping="Wrap" AcceptsTab="True" VerticalScrollBarVisibility="Auto" FontFamily="Segoe UI" FontSize="12" VerticalContentAlignment="Top" Template="{av:DynamicResource TextBoxControlTemplateMultiline}" av:Canvas.Left="4" av:Canvas.Top="0"><BSkyMultiLineLabel.Style><av:Style TargetType="av:TextBox"><av:Style.Resources><av:ResourceDictionary /></av:Style.Resources><av:Setter Property="av:UIElement.Effect"><av:Setter.Value><av:DropShadowEffect Color="#FFF5F5DC" /></av:Setter.Value></av:Setter></av:Style></BSkyMultiLineLabel.Style>Enter probabilities separated by a comma. Example : 0.3, 0.2, 0.5</BSkyMultiLineLabel><BSkyLabelReqdField Name="rq1" Text="*" Width="15" Height="25" Left="116" Top="9" CanExecute="False" Foreground="#FFFF0000" FontFamily="Segoe UI" FontSize="12" FontWeight="Bold" av:Canvas.Left="116" av:Canvas.Top="9">*</BSkyLabelReqdField><BSkyLabelReqdField Name="rq2" Text="*" Width="15" Height="25" Left="45" Top="75" CanExecute="False" Foreground="#FFFF0000" FontFamily="Segoe UI" FontSize="12" FontWeight="Bold" av:Canvas.Left="45" av:Canvas.Top="75">*</BSkyLabelReqdField><BSkyLabelReqdField Name="rq3" Text="*" Width="15" Height="25" Left="45" Top="137" CanExecute="False" Foreground="#FFFF0000" FontFamily="Segoe UI" FontSize="12" FontWeight="Bold" av:Canvas.Left="45" av:Canvas.Top="137">*</BSkyLabelReqdField></BSkyCanvas>