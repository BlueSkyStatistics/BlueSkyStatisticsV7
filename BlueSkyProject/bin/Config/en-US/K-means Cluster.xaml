<BSkyCanvas internalhelpfilename="{x:Null}" Title="KMeans Cluster" ModelClasses="{x:Null}" StatusTextBoxName="{x:Null}" customsyntax="{x:Null}" EditableComboBoxName="{x:Null}" RPackages="stats" CanExecute="True" PrereqCommandString="{x:Null}" CommandString="#Run the cluster analysis&#xA;BSkyKmeansRes&lt;-BSkyKMeans  (vars=c({{destination}}), centers={{clusters}}, iter.max = {{iterations}}, num.seeds = {{seeds}},storeClusterInDataset ={{storeClusterInDataset}}, varNameForCluster=&quot;{{variablename}}&quot;,dataset='{{%DATASET%}}') &#xA;#Display results in the output&#xA;BSkyFormat(BSkyKmeansRes)&#xA;#Bi plot&#xA;if ({{biplot}}) {BSkyBiPlot(vars=c({{destination}}), dataset='{{%DATASET%}}') }&#xA;#Store cluster numbers in the dataset&#xA;BSkyLoadRefreshDataframe({{%DATASET%}},{{storeClusterInDataset}})&#xA;&#xA;&#xA;" dlgsyntax="#Run the cluster analysis&#xA;BSkyKmeansRes&lt;-BSkyKMeans  (vars=c({{destination}}), centers={{clusters}}, iter.max = {{iterations}}, num.seeds = {{seeds}},storeClusterInDataset ={{storeClusterInDataset}}, varNameForCluster=&quot;{{variablename}}&quot;,dataset='{{%DATASET%}}') &#xA;#Display results in the output&#xA;BSkyFormat(BSkyKmeansRes)&#xA;#Bi plot&#xA;if ({{biplot}}) {BSkyBiPlot(vars=c({{destination}}), dataset='{{%DATASET%}}') }&#xA;#Store cluster numbers in the dataset&#xA;BSkyLoadRefreshDataframe({{%DATASET%}},{{storeClusterInDataset}})&#xA;&#xA;&#xA;" Helpfile="" splitProcessing="False" DatasetRequired="True" MenuLocation="{x:Null}" OutputDefinition="{x:Null}" Width="485" Command="False" Height="400" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 10px } .c2 { font-weight: bold; font-family: 'Lucida Grande'; font-size: 12px } .c3 { font-weight: normal; font-family: 'Lucida Grande'; font-size: 12px } .c4 { margin: 0px 0px 10px; font-size: 11px; font-family: 'Times New Roman' } .c5 { font-weight: normal; font-family: 'Lucida Grande'; font-size: 12px; background-color